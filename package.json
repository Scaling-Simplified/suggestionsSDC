{
  "name": "suggestion-service",
  "description": "module for shoe purchasing service",
  "author": "amelia-yeoh",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "nodemon server/start.js",
    "dev-react": "webpack",
    "react": "webpack --watch",
    "seed-database": "node db/seed.js",
    "test-be": "./node_modules/.bin/jest --config ./test/backend/jest.config.js",
    "test-fe": "./node_modules/.bin/jest --config ./test/frontend/jest.config.js",
    "test-be:watch": "npm run test-be -- --watch",
    "test-fe:watch": "npm run test-fe -- --watch",
    "test": "npm run test-fe && npm run test-be",
    "docker": "node db/seed.js && node server/start.js",
    "generate-cassandra-data": "node SDC/dataGenerator.js --lines 10000000 --output data.csv",
    "generate-postgres-suggestions": "node SDC/postgresSuggestionGenerator.js --lines 10000000 --output postgresSuggestions.csv",
    "generate-postgres-products": "node SDC/postgresProductGenerator.js --lines 10000000 --output postgresProduct.csv",
    "generate-indexing-psql": "node SDC/indexPostgresData.js --lines 10000000 --output indexingData.csv",
    "generate-psql-suggestions-storage": "node SDC/postgresSuggestionsStorage.js --lines 10000000 --output storageSuggestionsData.csv",
    "generate-psql-products-storage1": "node SDC/postgresProductsStorage.js --start 1 --lines 1000000 --output storageProductsData1.csv",
    "generate-psql-products-storage2": "node SDC/postgresProductsStorage.js --start 1000001 --lines 1000000 --output storageProductsData2.csv",
    "generate-psql-products-storage3": "node SDC/postgresProductsStorage.js --start 2000001 --lines 1000000 --output storageProductsData3.csv",
    "generate-psql-products-storage4": "node SDC/postgresProductsStorage.js --start 3000001 --lines 1000000 --output storageProductsData4.csv",
    "generate-psql-products-storage5": "node SDC/postgresProductsStorage.js --start 4000001 --lines 1000000 --output storageProductsData5.csv",
    "generate-psql-products-storage6": "node SDC/postgresProductsStorage.js --start 5000001 --lines 1000000 --output storageProductsData6.csv",
    "generate-psql-products-storage7": "node SDC/postgresProductsStorage.js --start 6000001 --lines 1000000 --output storageProductsData7.csv",
    "generate-psql-products-storage8": "node SDC/postgresProductsStorage.js --start 7000001 --lines 1000000 --output storageProductsData8.csv",
    "generate-psql-products-storage9": "node SDC/postgresProductsStorage.js --start 8000001 --lines 1000000 --output storageProductsData9.csv",
    "generate-psql-products-storage10": "node SDC/postgresProductsStorage.js --start 9000001 --lines 1000000 --output storageProductsData10.csv"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
